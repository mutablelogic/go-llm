{
  "description": "Message with function calls from model and function responses from user",
  "examples": [
    {
      "name": "Model function call",
      "message": {
        "role": "model",
        "parts": [
          {
            "text": "I'll check the weather for you."
          },
          {
            "function_call": {
              "name": "get_weather",
              "args": {
                "location": "San Francisco, CA",
                "unit": "fahrenheit"
              }
            }
          }
        ]
      }
    },
    {
      "name": "User function response",
      "message": {
        "role": "user",
        "parts": [
          {
            "function_response": {
              "name": "get_weather",
              "response": {
                "temperature": 65,
                "conditions": "sunny",
                "humidity": 70
              }
            }
          }
        ]
      }
    },
    {
      "name": "Multiple function calls",
      "message": {
        "role": "model",
        "parts": [
          {
            "function_call": {
              "name": "get_weather",
              "args": {
                "location": "New York"
              }
            }
          },
          {
            "function_call": {
              "name": "get_weather",
              "args": {
                "location": "Los Angeles"
              }
            }
          }
        ]
      }
    },
    {
      "name": "Light control example",
      "message": {
        "role": "model",
        "parts": [
          {
            "function_call": {
              "name": "set_light_values",
              "args": {
                "brightness": 25,
                "color_temp": "warm"
              }
            }
          }
        ]
      }
    },
    {
      "name": "Function response with result",
      "message": {
        "role": "user",
        "parts": [
          {
            "function_response": {
              "name": "set_light_values",
              "response": {
                "result": {
                  "brightness": 25,
                  "colorTemperature": "warm",
                  "status": "success"
                }
              }
            }
          }
        ]
      }
    }
  ]
}
