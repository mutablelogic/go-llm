{
  "description": "Simple text message - basic format for Gemini API",
  "examples": [
    {
      "name": "User text message",
      "message": {
        "role": "user",
        "parts": [
          {
            "text": "Hello, how are you?"
          }
        ]
      }
    },
    {
      "name": "Model response",
      "message": {
        "role": "model",
        "parts": [
          {
            "text": "I'm doing well, thank you for asking! How can I help you today?"
          }
        ]
      }
    },
    {
      "name": "Multi-part text message",
      "message": {
        "role": "user",
        "parts": [
          {
            "text": "Here's the first part of my message."
          },
          {
            "text": "And here's the second part."
          }
        ]
      }
    }
  ]
}
