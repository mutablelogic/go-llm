{
  "description": "Message with images for vision models (Pixtral)",
  "examples": [
    {
      "name": "Image with text (simple URL format)",
      "message": {
        "role": "user",
        "content": [
          {
            "type": "text",
            "text": "What's in this image?"
          },
          {
            "type": "image_url",
            "image_url": "https://docs.mistral.ai/img/eiffel-tower-paris.jpg"
          }
        ]
      }
    },
    {
      "name": "Image with text (object URL format)",
      "message": {
        "role": "user",
        "content": [
          {
            "type": "text",
            "text": "Describe this image"
          },
          {
            "type": "image_url",
            "image_url": {
              "url": "https://example.com/image.jpg"
            }
          }
        ]
      }
    },
    {
      "name": "Multiple images",
      "message": {
        "role": "user",
        "content": [
          {
            "type": "text",
            "text": "Compare these two images"
          },
          {
            "type": "image_url",
            "image_url": "https://example.com/image1.jpg"
          },
          {
            "type": "image_url",
            "image_url": "https://example.com/image2.jpg"
          }
        ]
      }
    },
    {
      "name": "Base64 encoded image",
      "message": {
        "role": "user",
        "content": [
          {
            "type": "text",
            "text": "What's in this image?"
          },
          {
            "type": "image_url",
            "image_url": {
              "url": "data:image/jpeg;base64,/9j/4AAQSkZJRg..."
            }
          }
        ]
      }
    },
    {
      "name": "Image without text",
      "message": {
        "role": "user",
        "content": [
          {
            "type": "image_url",
            "image_url": "https://example.com/image.jpg"
          }
        ]
      }
    }
  ]
}
